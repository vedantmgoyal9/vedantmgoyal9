{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/winget-pkgs-automation/schema.json",
  "Identifier": "Thunisoft.HuayuPY",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://pinyin.thunisoft.com/webapi/v1/setup/setupinfo?os=win",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "$Response = $Response.result",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".exe$",
  "PreviousVersion": "7.3.0.295",
  "ManifestFields": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "$Response.addr",
    "ReleaseDate": "$Response.date"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
