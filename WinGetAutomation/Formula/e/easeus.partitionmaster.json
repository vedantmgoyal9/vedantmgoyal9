  "AdditionalInfo": {
    "ForceUpgrade": false,
    "VersionRegex": "[0-9.]+",
    "InstallerRegex": ".exe$"
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
