{
  "$schema": "https://github.com/vedantmgoyal9/vedantmgoyal9/raw/main/WinGetAutomation/schema.json",
  "PackageIdentifier": "StandardNotes.StandardNotes",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://api.github.com/repos/standardnotes/app/releases?per_page=1",
      "Method": "Get",
      "Headers": {
        "Authorization": "\"Bearer $GithubBotToken\"",
        "Accept": "application/vnd.github.v3+json"
      },
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response | Where-Object { $_.name.Contains('Desktop') } | Select-Object -First 1",
  "Update": {
    "PackageVersion": "$Response.tag_name.TrimStart('@standardnotes/desktop@')",
    "InstallerUrls": "$Response.assets.Where({ $_.name -match '(x64|ia32)\\.exe$' }).browser_download_url"
  },
  "AdditionalInfo": {
    "PreRelease": false,
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
