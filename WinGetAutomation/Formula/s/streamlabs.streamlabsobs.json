{
  "$schema": "https://github.com/vedantmgoyal9/vedantmgoyal9/raw/main/WinGetAutomation/schema.json",
  "PackageIdentifier": "Streamlabs.StreamlabsOBS",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://api.github.com/repos/stream-labs/desktop/tags?per_page=1",
      "Method": "Get",
      "Headers": {
        "Authorization": "\"Bearer $env:GITHUB_BOT_TOKEN\"",
        "Accept": "application/vnd.github.v3+json"
      },
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": [
    "$Response = $Response.name.TrimStart('v')",
    "$UpdateCondition = -not $Response.Contains('-')"
  ],
  "Update": {
    "PackageVersion": "$Response",
    "InstallerUrls": "\"https://slobs-cdn.streamlabs.com/Streamlabs+OBS+Setup+$($Response).exe\""
  },
  "AdditionalInfo": {
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "$Formula.AdditionalInfo.PreviousReleaseId = $Response.id",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
