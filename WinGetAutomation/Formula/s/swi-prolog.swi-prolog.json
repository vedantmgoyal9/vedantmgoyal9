{
  "$schema": "https://github.com/vedantmgoyal9/vedantmgoyal9/raw/main/WinGetAutomation/schema.json",
  "PackageIdentifier": "SWI-Prolog.SWI-Prolog",
  "Source": [
    {
      "InvokeType": "WebRequest",
      "Uri": "https://www.swi-prolog.org/download/stable/bin/swipl-latest.x64.exe",
      "Method": "Head",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.BaseResponse.RequestMessage.RequestUri.OriginalString",
  "Update": {
    "PackageVersion": "($Response | Select-String -Pattern '(?<=-)[0-9.]+(?=-)').Matches.Value",
    "InstallerUrls": "@($Response, $Response.Replace('x64','x86'))"
  },
  "AdditionalInfo": {
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
