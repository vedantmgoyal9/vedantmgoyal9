{
  "$schema": "https://github.com/vedantmgoyal9/vedantmgoyal9/raw/main/WinGetAutomation/schema.json",
  "PackageIdentifier": "Plex.PlexMediaServer",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://plex.tv/api/downloads/5.json",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.computer.Windows",
  "Update": {
    "PackageVersion": "($Response.version | Select-String -Pattern '[0-9.]{2,}').Matches.Value",
    "InstallerUrls": "$Response.releases.url.Where({ $_ -match 'x86_64\\.exe$' })"
  },
  "AdditionalInfo": {
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
