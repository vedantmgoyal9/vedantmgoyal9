{
  "$schema": "https://github.com/vedantmgoyal9/vedantmgoyal9/raw/main/WinGetAutomation/schema.json",
  "PackageIdentifier": "Tencent.EDULite",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://sas.qq.com/cgi-bin/ke_download_speed",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.result.win.download_url",
  "Update": {
    "PackageVersion": "($Response | Select-String -Pattern '(?<=_)[0-9.]+(?=_)').Matches.Value",
    "InstallerUrls": "$Response"
  },
  "AdditionalInfo": {
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
