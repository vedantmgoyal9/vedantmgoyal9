{
  "$schema": "https://github.com/vedantmgoyal9/vedantmgoyal9/raw/main/WinGetAutomation/schema.json",
  "PackageIdentifier": "Tencent.QiDian",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://qidian.qq.com/store/qd_interface/Download.php?FType=0",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.data | Where-Object { $_.FPlatform -eq 1 }",
  "Update": {
    "PackageVersion": "($Response.FUrl | Select-String -Pattern '[0-9.]{3,}(?=\\.)').Matches.Value",
    "InstallerUrls": "$Response.FUrl",
    "ReleaseDate": "$Response.FReleaseTime"
  },
  "AdditionalInfo": {
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
