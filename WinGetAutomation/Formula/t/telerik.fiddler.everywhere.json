{
  "$schema": "https://github.com/vedantmgoyal9/vedantmgoyal9/raw/main/WinGetAutomation/schema.json",
  "PackageIdentifier": "Telerik.Fiddler.Everywhere",
  "Source": [
    {
      "InvokeType": "WebRequest",
      "Uri": "https://api.getfiddler.com/win/latest",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.BaseResponse.RequestMessage.RequestUri.OriginalString",
  "Update": {
    "PackageVersion": "($Response | Select-String -Pattern '(?<=%20)[0-9.]+(?=\\.)').Matches.Value",
    "InstallerUrls": "$Response"
  },
  "AdditionalInfo": {
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
