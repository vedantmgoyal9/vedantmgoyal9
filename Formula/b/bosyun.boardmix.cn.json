{
  "$schema": "https://github.com/vedantmgoyal9/winget-pkgs-automation/raw/main/schema.json",
  "PackageIdentifier": "Bosyun.BoardMix.CN",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://api.boardmix.cn/api/upgrade/desktop/bosyun/latest.yml",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response | ConvertFrom-Yaml",
  "Update": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "$Response.files[0].url",
    "ReleaseDate": "(Get-Date -Date $Response.releaseDate).ToString('yyyy-MM-dd')",
    "Locales": [
      {
        "Name": "zh-CN",
        "ReleaseNotes": "$Response.releaseNotes"
      }
    ]
  },
  "AdditionalInfo": {
    "ForceUpgrade": false,
    "VersionRegex": "[0-9.]+",
    "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$"
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
