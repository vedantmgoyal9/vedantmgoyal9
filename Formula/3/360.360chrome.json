{
  "$schema": "https://github.com/vedantmgoyal9/winget-pkgs-automation/raw/main/schema.json",
  "PackageIdentifier": "360.360Chrome",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://chrome.360.cn/m/ver.ini",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.Split(':')[1]",
  "Update": {
    "PackageVersion": "$Response",
    "InstallerUrls": "\"https://down.360safe.com/cse/360cse_$($Response).exe\""
  },
  "AdditionalInfo": {
    "ForceUpgrade": false,
    "VersionRegex": "[0-9.]+",
    "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$"
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
