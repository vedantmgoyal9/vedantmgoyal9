{
  "$schema": "https://github.com/vedantmgoyal9/winget-pkgs-automation/raw/main/schema.json",
  "PackageIdentifier": "DaxStudio.DaxStudio",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://api.github.com/repos/DaxStudio/DaxStudio/releases?per_page=1",
      "Method": "Get",
      "Headers": {
        "Authorization": "\"Bearer $GithubBotToken\"",
        "Accept": "application/vnd.github.v3+json"
      },
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$UpdateCondition = $Response.prerelease -eq $PreRelease",
  "Update": {
    "PackageVersion": "$Response.tag_name.TrimStart('v')",
    "InstallerUrls": {
      "x64|user": "$Response.assets.Where({ $_.name -match '.exe$' }).browser_download_url",
      "x64|machine": "$Response.assets.Where({ $_.name -match '.exe$' }).browser_download_url"
    }
  },
  "AdditionalInfo": {
    "PreRelease": false,
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
