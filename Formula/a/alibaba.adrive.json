{
  "$schema": "https://github.com/vedantmgoyal9/winget-pkgs-automation/raw/main/schema.json",
  "PackageIdentifier": "Alibaba.aDrive",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://www.aliyundrive.com/desktop/version/update.json",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = Invoke-RestMethod -Uri \"$($Response.url)/win32/ia32/latest.yml\" | ConvertFrom-Yaml",
  "Update": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "$Response.files[0].url",
    "ReleaseDate": "(Get-Date -Date $Response.releaseDate).ToString('yyyy-MM-dd')"
  },
  "AdditionalInfo": {
    "ForceUpgrade": false,
    "VersionRegex": "[0-9.]+",
    "InstallerRegex": ".exe$"
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
