{
  "$schema": "https://github.com/vedantmgoyal9/winget-pkgs-automation/raw/main/schema.json",
  "PackageIdentifier": "angryziber.AngryIPScanner",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://api.github.com/repos/angryip/ipscan/releases?per_page=1",
      "Method": "Get",
      "Headers": {
        "Authorization": "\"Bearer $GithubBotToken\"",
        "Accept": "application/vnd.github.v3+json"
      },
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$UpdateCondition = $Response.prerelease -eq $PreRelease",
  "Update": {
    "PackageVersion": "$Response.tag_name.TrimStart('v')",
    "InstallerUrls": "$Response.assets.Where({ $_.name -match 'setup.exe$' }).browser_download_url + '|neutral'"
  },
  "AdditionalInfo": {
    "PreRelease": false,
    "ForceUpgrade": false,
    "VersionRegex": "(?<=v)[0-9.]+",
    "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$"
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
