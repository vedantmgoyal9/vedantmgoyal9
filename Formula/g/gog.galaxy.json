    "PackageVersion": "$Response.version",
    "InstallerUrls": "(Invoke-WebRequest $Response.downloadLink -Method Head).BaseResponse.RequestMessage.RequestUri.OriginalString"
  },
  "AdditionalInfo": {
    "ForceUpgrade": false
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
