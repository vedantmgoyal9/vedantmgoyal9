{
  "$schema": "https://github.com/vedantmgoyal2009/winget-pkgs-automation/raw/main/schema.json",
  "Identifier": "Microsoft.AzureCosmosEmulator",
  "Update": {
    "InvokeType": "WebRequest",
    "Uri": "https://aka.ms/cosmosdb-emulator",
    "Method": "Head",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "$Response = $Response.BaseResponse.RequestMessage.RequestUri.OriginalString",
  "VersionRegex": "[0-9.]{6,}",
  "InstallerRegex": ".msi$",
  "PreviousVersion": "",
  "ManifestFields": {
    "PackageVersion": "($Response | Select-String -Pattern $VersionRegex).Matches.Value",
    "InstallerUrls": "$Response"
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
