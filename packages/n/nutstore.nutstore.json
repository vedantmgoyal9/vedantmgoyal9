{
  "$schema": "https://github.com/vedantmgoyal2009/winget-pkgs-automation/raw/main/schema.json",
  "Identifier": "Nutstore.Nutstore",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://app.jianguoyun.com/static/exe/latestVersion",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": "NutstoreApp-6.2.4"
  },
  "PostResponseScript": "$Response = ($Response | Where-Object { $_.OS -eq 'win-wpf-client' }).stVer",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".exe$",
  "PreviousVersion": "",
  "ManifestFields": {
    "PackageVersion": "$Response",
    "InstallerUrls": "\"https://www.jianguoyun.com/static/exe/installer/$($Response)/NutstoreInstaller_$($Response).exe\""
  },
  "AdditionalInfo": {},
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "AutoUpgrade": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
