{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/src/winget-pkgs-automation/schema.json",
  "Identifier": "OpenMedia.4KVideoDownloader",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://dl.4kdownload.com/app/appcast/videodownloader.xml",
    "Method": "Get",
    "Headers": {},
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": [
    "$Response = $Response[0]",
    "$Response2 = ($Response.title | Select-String -Pattern $VersionRegex).Matches.Value"
  ],
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": ".(exe|msi|msix|appx)(bundle){0,1}$",
  "PreviousVersion": "",
  "ManifestFields": {
    "PackageVersion": "$Response.version",
    "InstallerUrls": "@($x64_Uri.Replace('#version', $Response2), $x86_Uri.Replace('#version', $Response2), $x64_Online_Uri.Replace('#version', $Response2), $x86_Online_Uri.Replace('#version', $Response2))",
    "ReleaseDate": "(Get-Date -Date $Response.pubDate).ToString('yyyy-MM-dd')"
  },
  "AdditionalInfo": {
    "x64_Uri": "https://dl.4kdownload.com/app/4kvideodownloader_#version_x64.msi",
    "x86_Uri": "https://dl.4kdownload.com/app/4kvideodownloader_#version.msi",
    "x64_Online_Uri": "https://dl.4kdownload.com/app/4kvideodownloader_#version_x64_online.exe",
    "x86_Online_Uri": "https://dl.4kdownload.com/app/4kvideodownloader_#version_online.exe"
  },
  "PostUpgradeScript": "",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
