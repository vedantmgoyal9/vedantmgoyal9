{
  "$schema": "https://github.com/vedantmgoyal2009/vedantmgoyal2009/raw/main/src/winget-pkgs-automation/schema.json",
  "Identifier": "ArtifexSoftware.GhostScript",
  "Update": {
    "InvokeType": "RestMethod",
    "Uri": "https://api.github.com/repos/ArtifexSoftware/ghostpdl-downloads/releases?per_page=1",
    "Method": "Get",
    "Headers": {
      "Authorization": "$AuthToken",
      "Accept": "application/vnd.github.v3+json"
    },
    "Body": "",
    "UserAgent": ""
  },
  "PostResponseScript": "$UpdateCondition = $Response.prerelease -eq $PreRelease -and $Response.id -gt $PreviousReleaseId",
  "VersionRegex": "[0-9.]+",
  "InstallerRegex": "w64.(exe|msi|msix|appx)(bundle){0,1}$",
  "PreviousVersion": "9.56.1",
  "ManifestFields": {
    "PackageVersion": "($Response.name | Select-String -Pattern $VersionRegex).Matches.Value",
    "InstallerUrls": "$Response.assets | ForEach-Object { if ($_.name -match $InstallerRegex) { $_.browser_download_url } }",
    "ReleaseNotesUrl": "$Response.html_url",
    "ReleaseDate": "(Get-Date -Date $Response.published_at).ToString('yyyy-MM-dd')"
  },
  "AdditionalInfo": {
    "PreRelease": false,
    "PreviousReleaseId": 63531246
  },
  "PostUpgradeScript": "$Package.AdditionalInfo.PreviousReleaseId = $Response.id",
  "YamlCreateParams": {
    "SkipPRCheck": false,
    "DeletePreviousVersion": false
  },
  "SkipPackage": false
}
