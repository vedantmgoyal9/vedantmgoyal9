{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "functions": {
    "api/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/gh-release-and-repo-info/:owner/:repo/:release_tag",
      "destination": "/api/gh-release-and-repo-info"
    },
    {
      "source": "/api/winget-pkgs/versions:gh(_github)?/:package_identifier",
      "destination": "/api/winget-pkgs/versions:gh?package_identifier=:package_identifier"
    },
    {
      "source": "/api/winget-pkgs/manifests/:package_identifier/:version?",
      "destination": "/api/winget-pkgs/manifests"
    },
    {
      "source": "/api/winget-pkgs-rest/packageManifests/:package_identifier",
      "destination": "/api/winget-pkgs-rest/packageManifests"
    }
  ],
  "public": false,
  "github": {
    "autoJobCancelation": true
  },
  "trailingSlash": false
}
